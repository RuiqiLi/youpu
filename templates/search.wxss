.search-wrapper{
  background: white;
  box-sizing: border-box;
  height: 100rpx;
  padding: 18rpx 15rpx;
  border-bottom: 1rpx solid #f2f2f2;
}
.search{
  height: 100%;
  width: 100%;
  background: #f2f2f2;
  border-radius: 15rpx;
  display: flex;
}
.search-icon{
  margin: 18rpx 15rpx;
}
.search-input{
  margin-right: 15rpx;
  height: 100%;
  line-height: 100%;
  width: 100%;
  color: #353535;
  font-size: 13px;
}